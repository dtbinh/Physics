#
# parameters
#
#

# parser to use
parser: "version 0.1"

# name
name: "rubens"

# collision category
category: 1

# offsets
offset: 3.23 -2.1 0.9

# conv from rvs
conv from rvs: 0.5

# root node
root node: "pelvis"

#
# parts definitions
#
# syntax:
#   sphere part: name x y z mass massadj radius isStriker isStriked
#   ccylinder part: name x y z mass massadj radius length isStriker isStriked
#   box part: name x y z mass massadj xlen ylen zlen isStriker isStriked
#

box part: "head" 0.000000 0.000000 1.650000 5.000000 5.000000 0.150000 0.150000 0.200000 0 0
box part: "upper_arm_left" 0.000000 0.170000 1.325000 5.000000 5.000000 0.150000 0.150000 0.330000 0 0
box part: "upper_arm_right" 0.000000 -0.170000 1.325000 5.000000 5.000000 0.150000 0.150000 0.330000 0 0
box part: "forearm_left" 0.000000 0.170000 1.060000 5.000000 5.000000 0.150000 0.150000 0.200000 0 0
box part: "forearm_right" 0.000000 -0.170000 1.060000 5.000000 5.000000 0.150000 0.150000 0.200000 0 0
box part: "chest" 0.000000 0.000000 1.440000 5.000000 5.000000 0.150000 0.150000 0.220000 0 0
box part: "stomach" 0.000000 0.000000 1.230000 5.000000 5.000000 0.150000 0.150000 0.200000 0 0
box part: "pelvis" 0.003000 0.000000 1.056000 5.000000 5.000000 0.150000 0.252750 0.150000 0 0
box part: "thigh_left" 0.000000 0.126500 0.750000 5.000000 5.000000 0.150000 0.150000 0.420000 0 0
box part: "thigh_right" 0.000000 -0.126500 0.750000 5.000000 5.000000 0.150000 0.150000 0.420000 0 0
