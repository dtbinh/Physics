box part: "hand_right" 0.000000 -0.170000 0.868000 5.000000 5.000000 0.150000 0.150000 0.184000 0 0

#
# joint definitions
#
# syntax:
#   ball joint: name part1 part2 x y z
#   fixed joint: name part1 part2
#
# note:
#   part1 is inbound, part2 is outbound, search only go from inbound
#   to outbound, e.g. not every part can be rootnode unless you modify
#   joint in-out bound part order.
#

ball joint: "joint_pelvis_thigh_left" "pelvis" "thigh_left" 0.000000 0.120000 0.960000
ball joint: "joint_thigh_left_shank_left" "thigh_left" "shank_left" 0.000000 0.133000 0.540000
ball joint: "joint_shank_left_foot_left" "shank_left" "foot_left" 0.003000 0.133000 0.060000
